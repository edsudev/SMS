    <div id="content" class="main-content">
        <div class="layout-px-spacing">
            <div class="row invoice layout-top-spacing layout-spacing">
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">

                    <div class="doc-container">

                        <div class="row">

                            <div class="col-xl-9">

                                <div class="invoice-container">
                                    <div class="invoice-inbox">

                                        <div id="ct" class="">

                                            <div class="invoice-00001">
                                                <div class="content-section">

                                                    <div class="inv--head-section inv--detail-section">
                                                        <div class="row">
                                                            <div class="col-sm-12 col-xl-12 col-lg-12 col-md-12 justify-content-around mx-auto">
                                                                <div class="d-flex">
                                                                    <img src="~/logo.png" style="width:150px; height:130px" class="mt-2" alt="Logo" />
                                                                    <div class="my-auto text-center">
                                                                        <h2 class="in-heading font-bold">EDO STATE UNIVERSITY UZAIRUE</h2>
                                                                        <h5 class="in-heading">@Model.YearOfAdmission ADMISSION CLEARANCE</h5>
                                                                        <h6 class="">CANDIDATE'S APPLICATION PRINTOUT</h6>
                                                                    </div>
                                                                    <img src="~/files/applicantUploads/passports/@Model.PassportUpload" style="width:150px; height:130px" class="mt-2" alt="avatar" />
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="inv--detail-section inv--customer-detail-section">

                                                        <div class="row">
                                                            <div class="col-7">
                                                                <h5>BIO DATA</h5>
                                                                <div class="row">
                                                                    <div class="col-xl-2 col-lg-2 col-md-2 col-sm-2 col-2">
                                                                        <p><span class=" inv-subtitle">Name: </span></p>
                                                                        <p><span class=" inv-subtitle">Sex: </span></p>
                                                                        <p><span class=" inv-subtitle">DOB:</span></p>
                                                                        <p><span class=" inv-subtitle">Nationality:  </span></p>
                                                                        <p><span class=" inv-subtitle">State:</span></p>
                                                                        <p><span class=" inv-subtitle">LGA:</span></p>
                                                                        <p><span class=" inv-subtitle">E-mail: </span></p>
                                                                        <p><span class=" inv-subtitle">Address: </span></p>
                                                                    </div>
                                                                    <div class="col-xl-10 col-lg-10 col-md-10 col-sm-10 col-2">
                                                                        <p><span class=" inv-subtitle">@Model.Surname @Model.FirstName @Model.OtherName </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.Sex </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.DOB </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.Nationalities.Name </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.States.Name </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.LGAs.Name </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.Email </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.ContactAddress</span></p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-5">
                                                                <h5>ADMISSION</h5>
                                                                <div class="row">
                                                                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-4 col-4">
                                                                        <p><span class=" inv-subtitle">Mode: </span></p>
                                                                        <p><span class=" inv-subtitle">Reg No.: </span></p>
                                                                        <p><span class=" inv-subtitle">Score:</span></p>
                                                                        <p><span class=" inv-subtitle">Course 1:  </span></p>
                                                                        <p><span class=" inv-subtitle">Course 2:  </span></p>
                                                                        <p><span class=" inv-subtitle">Course 3:  </span></p>

                                                                    </div>
                                                                    <div class="col-xl-8 col-lg-8 col-md-8 col-sm-8 col-8">
                                                                        <p><span class=" inv-subtitle">@Model.ModeOfEntry </span></p>
                                                                        
                                                                        <p><span class=" inv-subtitle">Score</span></p>
                                                                        <p><span class=" inv-subtitle">@Model.FirstChoice</span></p>
                                                                        <p><span class=" inv-subtitle">@Model.SecondChoice </span></p>
                                                                        <p><span class=" inv-subtitle">@Model.ThirdChoice</span></p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                    
                                                    <div class="">
                                                        <h6 class="text-center">SSCE RESULT(S)</h6>

                                                        <div class="table-responsive">
                                                            <table class="table">
                                                                <thead>
                                                                    <tr>
                                                                        <th>@Model.Ssce1Type @Model.Ssce1Year</th>
                                                                        <th>Exam Number: @Model.Ssce1Number</th>
                                                                    </tr>
                                                                    <tr>
                                                                        <th>Subject</th>
                                                                        <th>Grade</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject1 </td>
                                                                        <td>@Model.Ssce1Subject1Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject2 </td>
                                                                        <td>@Model.Ssce1Subject2Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject3 </td>
                                                                        <td>@Model.Ssce1Subject3Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject4 </td>
                                                                        <td>@Model.Ssce1Subject4Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject5 </td>
                                                                        <td>@Model.Ssce1Subject5Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject6 </td>
                                                                        <td>@Model.Ssce1Subject6Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject6 </td>
                                                                        <td>@Model.Ssce1Subject6Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject7 </td>
                                                                        <td>@Model.Ssce1Subject7Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject8 </td>
                                                                        <td>@Model.Ssce1Subject8Grade</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td>@Model.Ssce1Subject9 </td>
                                                                        <td>@Model.Ssce1Subject9Grade</td>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                    
                                                    <div class="inv--note">

                                                        <div class="row">
                                                            <div class="col-sm-12 col-12 order-sm-0 order-1">
                                                                <p>Powered by EDSU ICT UNIT</p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                            </div>

                            <div class="col-xl-3">

                                <div class="invoice-actions-btn">

                                    <div class="invoice-action-btn">

                                        <div class="row">

                                            <div class="col-xl-12 col-md-3 col-sm-6">
                                                <a href="javascript:void(0);" class="btn btn-secondary btn-print  action-print">Print</a>
                                            </div>

                                        </div>
                                    </div>

                                </div>

                            </div>


                        </div>


                    </div>

                </div>
            </div>
        </div>
       
